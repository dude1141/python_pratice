a1 = [{
  'jdbc_1': {
    'CHECK_records': 'N',
    'abcd': 'Y',
    'MNB': 'N',
    'OST': 'N'}
    }]


for i in a1:
    for key,val in i.items():
        print(val)
        print(val.get('CHECK_records'))

